create table price_report (
  source varchar2(100),
  price varchar2(100),
  promotion varchar2(200),
  promotion_url varchar2(100),
  p_date varchar2(100),
  agency_name varchar2(100),
  sales_area varchar2(100),
  province varchar2(100),
  city varchar2(100),
  carseries_name varchar2(100),
  b_name varchar2(100),
  mb_name varchar2(100),
  cartype_name varchar2(100),
  total_price varchar2(100),
  bare_price varchar2(100),
  purchase_tax varchar2(100),
  insurance varchar2(100),
  use_tax varchar2(100),
  card_price varchar2(100),
  old_price varchar2(100),
  compulsoryInsurance_price varchar2(100),
  crawl_date varchar2(100),
  promotion_price varchar2(100),
  p_level varchar2(100),
  model_year varchar2(100),
  source2 varchar2(100),
  MSRP varchar2(100),
  gap varchar2(100),
  source_id varchar2(100),
  mac varchar2(100),
  is_4s varchar2(100),
  dealerName varchar2(100)
);
